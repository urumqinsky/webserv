server   {
	ip 127.0.0.1;
	port 4444;
	root /Users/rlando/Desktop/webserv;
	server_name localhost;

	location /  {
		allow GET;
		autoindex on;
		max_body_size 55;
		root /Users/rlando/Desktop/webserv/cgi-bin/names/
		cgi ./names;
		cgi_extension .py;
	}
	location /nginx_test  {
		allow GET;
		autoindex on;
		max_body_size 55;
		autoindex on;
	}

	location /nginx_test/data  {
		allow GET POST;
		autoindex on;
		authorization on;
		max_body_size 55;
	}
}
error_page 500 501 502 /web/errors/5.html;
error_page 404 /web/errors/404.html;
error_page 401 /web/errors/401.html;
