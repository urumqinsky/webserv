server   {
	ip 127.0.0.1;
	port 4444;
	root /Users/heula/webserv/;

	server_name localhost;
	location / {
		allow GET PUT POST;
		autoindex on;
		index index.html;
		cgi ./cgi;
		cgi_extension .bla;
		max_body_size 5;
	}

	location /nginx_test  {
		allow GET;
		autoindex on;
		root /Users/heula/webserv/;
		max_body_size 55;
		autoindex on;
	}
		location /nginx_test/data  {
		allow GET;
		autoindex on;
		root /Users/heula/webserv;
		max_body_size 55;

	}
	location /configFile1  {
		allow GET;
		autoindex on;
		root /Users/heula/webserv;
		max_body_size 55;
	}

}
error_page 500 501 502 /errors/5.html;
error_page 404 /errors/404.html;
root /Users/heula/webserv
max_body_size 100;

