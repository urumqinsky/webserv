server   {
	ip 127.0.0.1;
	port 4444;

	server_name localhost;
	location / {
		allow GET;
		autoindex on;
		index index.html;
		alias ./testers/YoupiBanane;
		cgi ./helloo;
		cgi_extension .cprog;
	}

	location /  {
		index index.html
		allow GET;
		autoindex on;
		root /Users/ltulune/Desktop/webserv;
		max_body_size 55;
		cgi ./;
		cgi_extension .cprog;
	}
	location /nginx_test  {
		index login.html;
		allow GET;

		max_body_size 55;
		authorization on;
	}
		location /nginx_test/data  {
		allow GET;
		autoindex on;
		root /Users/ltulune/Desktop/webserv;
		max_body_size 55;

	}
	location /configFile1  {
		allow GET;
		autoindex on;
		root /Users/ltulune/Desktop/webserv;
		max_body_size 55;
	}

}
error_page 500 501 502 /errors/5.html;
error_page 404 /errors/404.html;
root /Users/ltulune/Desktop/webserv
